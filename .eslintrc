{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "import/no-webpack-loader-syntax": "off",
    "global-require": "off",
    "no-param-reassign": "off"
  },
  "settings": {
    "import/resolver": "webpack"
  }
}
