{
  "extends": "airbnb",
  "env": {
    "node": true
  },
  "rules": {
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "no-param-reassign": ["error", { "props": false }],
    "no-underscore-dangle": "off",
    "no-console": "off",
    "no-multi-assign": "off",
    "no-prototype-builtins": "off"
  }
}
